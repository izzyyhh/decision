{
    "name": "mmp3-api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": "14.16.1"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "npm run prebuild && dotenv -- nest start",
        "start:dev": "npm run prebuild && dotenv -- nest start --watch --preserveWatchOutput",
        "start:debug": "npm run prebuild && dotenv -- nest start --debug --watch --preserveWatchOutput",
        "start:prod": "node dist/main",
        "lint": "npm run lint:eslint && npm run lint:tsc",
        "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx,.json,.md --max-warnings 0 src/",
        "lint:tsc": "tsc --project ./tsconfig.lint.json",
        "console": "dotenv -- ts-node --transpile-only -r tsconfig-paths/register src/console.ts",
        "console:prod": "node dist/console.js",
        "mikro-orm": "mikro-orm",
        "mikro-orm:drop": "mikro-orm schema:drop -r",
        "mikro-orm:migration:generate": "mikro-orm migration:create",
        "db:migrate:prod": "npm run console:prod migrate --",
        "db:migrate": "npm run console migrate --",
        "fixtures:prod": "npm run console:prod fixtures --",
        "fixtures": "npm run console fixtures --"
    },
    "dependencies": {
        "@mikro-orm/cli": "^4.5.10",
        "@mikro-orm/core": "^4.5.10",
        "@mikro-orm/nestjs": "^4.3.1",
        "@mikro-orm/postgresql": "^4.5.10",
        "@nestjs/common": "^8.0.1",
        "@nestjs/config": "^1.1.6",
        "@nestjs/core": "^8.2.6",
        "@nestjs/graphql": "^9.0.4",
        "@nestjs/passport": "^8.1.0",
        "@nestjs/platform-express": "^8.2.6",
        "apollo-server-core": "^3.3.0",
        "apollo-server-express": "^3.3.0",
        "async": "^3.2.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cli-progress": "^3.10.0",
        "commander": "^8.3.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "dotenv-cli": "^4.1.1",
        "elastic-apm-node": "^3.27.0",
        "faker": "^5.4.0",
        "firebase-admin": "^10.0.2",
        "graphql": "^15.5.0",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "minio": "^7.0.26",
        "nestjs-console": "^7.0.1",
        "node-cache": "^5.1.2",
        "node-fetch": "^3.2.0",
        "passport": "^0.5.2",
        "passport-http-bearer": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.1",
        "pg-error-constants": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.2",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.2.0",
        "@nestjs/schematics": "^8.0.5",
        "@nestjs/testing": "^8.2.6",
        "@types/async": "^3.2.12",
        "@types/cli-progress": "^3.9.2",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/faker": "^5.1.6",
        "@types/minio": "^7.0.11",
        "@types/node": "^17.0.12",
        "@types/node-fetch": "^3.0.2",
        "@types/passport-http-bearer": "^1.0.37",
        "@types/passport-jwt": "^3.0.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./src/db/ormconfig.cli.ts",
            "./dist/db/ormconfig.cli.js"
        ]
    }
}
